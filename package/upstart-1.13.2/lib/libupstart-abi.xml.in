<descriptor>

<version>
	@libupstart_version@-build
</version>

<headers>
	@abs_builddir@
</headers>

<libs>
	@abs_builddir@
</libs>

<!--
  Include paths are automatically detected but the tool needs help to
  find the default include directory.
-->
<add_include_paths>
	/usr/include
</add_include_paths>

</descriptor>
